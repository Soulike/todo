{
  "name": "todo",
  "version": "0.1.0",
  "description": "My todo app.",
  "author": "soulike",
  "main": "./public/electron.js",
  "homepage": ".",
  "private": true,
  "proxy": "http://39.96.200.130:8003/",
  "dependencies": {
    "@types/faker": "^4.1.5",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^24.0.18",
    "@types/mathjax": "^0.0.35",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/showdown": "^1.9.3"
  },
  "devDependencies": {
    "antd": "^3.23.0",
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "electron": "^6.0.8",
    "electron-builder": "^21.2.0",
    "faker": "^4.1.0",
    "highlight.js": "^9.15.10",
    "mathjax": "^2.7.5",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "showdown": "^1.9.0",
    "source-map-explorer": "^2.0.1",
    "typescript": "3.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-dev": "react-scripts build",
    "build": "react-scripts build && rm build/static/js/*.map && rm build/static/css/*.map",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-react": "NODE_ENV=production react-app-rewired start",
    "start-app": "NODE_ENV=development electron .",
    "prebuild-app": "NODE_ENV=production react-app-rewired build && rm build/static/js/*.map && rm build/static/css/*.map && rm -rf dist",
    "build-app": "electron-builder"
  },
  "build": {
    "appId": "tech.soulike.todo",
    "productName": "Todo",
    "icon": "public/avatar.png",
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg",
      "darkModeSupport": true
    },
    "win": {
      "target": "portable"
    },
    "files": [
      "**/*",
      "!node_modules"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
